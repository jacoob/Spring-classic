<?xml version="1.0" encoding="UTF-8"?>
<beans
        xmlns="http://www.springframework.org/schema/beans"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns:p="http://www.springframework.org/schema/p"
        xsi:schemaLocation="http://www.springframework.org/schema/beans
               http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">


    <!--    Dependency Injection with static Factory  Method in same class in Spring-->
    <bean id="a" class="dependencyInjection.factorymethod.statick.insameclass.A" factory-method="getA"/>

    <!--    Dependency Injection with static Factory Method in other class in Spring-->
    <bean id="printablebeanA" class="dependencyInjection.factorymethod.statick.inotherclass.PrintFactory" factory-method="getPrintableA"/>
    <bean id="printablebeanB" class="dependencyInjection.factorymethod.statick.inotherclass.PrintFactory" factory-method="getPrintableB"/>

    <!--    Dependency Injection with non static Factory Method in same class in Spring-->
    <bean id="pf" class="dependencyInjection.factorymethod.nonstatic.PrintFactory" />
    <bean id="pfa" class="dependencyInjection.factorymethod.nonstatic.PrintFactory" factory-bean="pf" factory-method="getPrintableA"/>


</beans>